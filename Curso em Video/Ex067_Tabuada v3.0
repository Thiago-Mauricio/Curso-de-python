#tabuado interrompida com n = número negativo
while True:
    n = int(input('Digite um número inteiro positivo para saber sua tabuada: '))
    if n < 0:
        break
    print('-' * 30)
    for t in range(1,11):
        print(f'{n}  X {t:2} = {n*t:2}')
    print('-' * 30)
print('PROGRAMA ENCERRADO !')